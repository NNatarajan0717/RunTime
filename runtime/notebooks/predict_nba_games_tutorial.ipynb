    "def add_target(team):\n",
    "    new_team_df = team.copy()\n",
    "    new_team_df[\"target\"] = new_team_df[\"won\"].shift(-1)\n",
    "    return new_team_df\n",
    "\n",
    "df = df.groupby(\"team\", group_keys=False).apply(add_target)"
   ]
